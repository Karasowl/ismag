{
  "buildCommand": "remix vite:build",
  "devCommand": "remix vite:dev",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "build/client",
  "functions": {
    "api/index.js": {
      "includeFiles": "build/server/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
